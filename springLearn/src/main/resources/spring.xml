<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"

       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
http://www.springframework.org/schema/context  http://www.springframework.org/schema/context/spring-context-4.3.xsd">
    <bean id="testBean" class="com.lww.spring.learn.bean.TestBean" >
    </bean>
    <context:annotation-config/>
    <bean id="mailBean" class="com.lww.spring.learn.bean.BaseMailBean">
        <property name="sendMail" value="asda"/>
        <property name="protocol" value="a"/>
        <property name="sendMailPassWord" value="adad"/>
        <property name="sendHostServer" value="adssad"/>
    </bean>

</beans>